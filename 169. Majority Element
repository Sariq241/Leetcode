class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int n = nums.size();
        int count=0;
        int sum;
        for(int i=0; i<n; i++){
           if(count == 0){
            sum =nums[i];
            count ++;
           }
           else if(nums[i]== sum){
            count ++;
           }
           else{
            count--;
           }
        }
      return sum;
    }
};
