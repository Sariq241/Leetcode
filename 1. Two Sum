class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int sum;
        int n=nums.size();
        for(int i=0; i<n; i++){
            for(int j =i + 1; j<n; j++){
               sum = nums[i]+nums[j];
               if (sum == target){
            
        
         return {i, j};
        } 
            }
        }
        throw std::invalid_argument("No two sum solution found");
    }
};
