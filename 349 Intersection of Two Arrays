int* intersection(int* nums1, int nums1Size, int* nums2, int nums2Size, int* returnSize) {
    int arr[1001] = {0}; 
    int* arr2 = (int*)malloc(sizeof(int) * nums1Size); 
    int j = 0;
    for (int i = 0; i < nums1Size; i++) {
        arr[nums1[i]] = 1;
    }
    for (int i = 0; i < nums2Size; i++) {
        if (arr[nums2[i]] == 1) {
            arr[nums2[i]] = 0; 
            arr2[j++] = nums2[i];
        }
    }
    *returnSize = j;
    return arr2;
}
